isbn = {1097-0134},
issn = {08873585},
journal = {Proteins: Structure, Function and Bioinformatics},
keywords = {Model aggregation,PKa,Prediction,Statistics,Titration,bayes,bma,machine learning,pka,prediction,statistics,titration},
