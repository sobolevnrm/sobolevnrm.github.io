doi = {10.1021/ct401028g},
file = {:C$\backslash$:/Users/bake113/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Daily et al. - 2014 - Improved Coarse-Grained Modeling of Cholesterol-Containing Lipid Bilayers(2).pdf:pdf},
issn = {1549-9618},
journal = {Journal of chemical theory and computation},
keywords = {biomembranes,cholesterol,coarse graining,force fields,molecular simulation,sterols},
