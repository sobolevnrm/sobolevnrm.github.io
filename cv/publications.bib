issn = {1520-5207},
journal = {The journal of physical chemistry. B},
month = {may},
pages = {acs.jpcb.6b02059},
pmid = {27089174},
title = {{Energy Minimization of Discrete Protein Titration State Models Using Graph Theory.}},
url = {http://arxiv.org/abs/1507.07021 http://pubs.acs.org/doi/abs/10.1021/acs.jpcb.6b02059 http://www.ncbi.nlm.nih.gov/pubmed/27089174},
year = {2016}
}
