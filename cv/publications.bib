file = {:C$\backslash$:/Users/bake113/Documents/References/Harper et al/2013/Harper et al. - 2013 - Nanoinformatics workshop report Current resources, community needs, and the proposal of a collaborative framework.pdf:pdf},
isbn = {1749-4699},
